    	"adminapi.domainCode": 1,
    	"adminapi.lang": "langEN",
		"expected.httpStatus.response":200
     },
    {
		"test-name": "PublicController_loadSubDomains_dataIn: non viene passato parametro mandatario [domainCode]",
